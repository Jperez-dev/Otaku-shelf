{
  "functions": {
    "api/image-proxy.js": {
      "maxDuration": 30
    },
    "api/manga/index.js": {
      "maxDuration": 30
    },
    "api/manga/[id].js": {
      "maxDuration": 30
    },
    "api/statistics/manga/index.js": {
      "maxDuration": 30
    },
    "api/statistics/manga/[id].js": {
      "maxDuration": 30
    },
    "api/chapter/index.js": {
      "maxDuration": 30
    },
    "api/chapter/[id].js": {
      "maxDuration": 30
    },
    "api/at-home/server/[id].js": {
      "maxDuration": 30
    }
  },
  "rewrites": [
    {
      "source": "/covers/:mangaId/:filename*",
      "destination": "https://uploads.mangadex.org/covers/:mangaId/:filename*"
    },
    {
      "source": "/((?!api/).*)",
      "destination": "/index.html"
    }
  ]
}